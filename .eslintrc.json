{
    "root": true,
    "extends": ["airbnb"],
    "plugins": ["prettier", "import"],
    "parserOptions": {
      "ecmaVersion": 2016,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "node": true,
      "mocha": true
    },
    "rules": {
      "no-console": "error",
      "comma-dangle": "off"
    }
}
  